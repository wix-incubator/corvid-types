{
  "name": "corvid-types",
  "version": "0.1.87",
  "description": "Type definitions for Corvid by Wix",
  "scripts": {
    "lint": "eslint .",
    "build": ":",
    "test": "jest",
    "generate-dts": "docworks dts -r git@github.com:wix-private/wix-code-docs.git -o declaration -d temp_declarations --wixselector && npm run copy-declarations temp_declarations",
    "postpublish" : "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
    "copy-declarations": "scripts/copy_declarations.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix-incubator/corvid-types"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "main": "src/index.js",
  "files": [
    "types",
    "configs"
  ],
  "types": "./types/index.d.ts",
  "keywords": [
    "corvid",
    "corvid-cli",
    "wix"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@commitlint/travis-cli": "^8.1.0",
    "docworks-cli": "^1.10.2",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-prettier": "^3.1.0",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "dependencies": {
    "@types/node": "^12.7.5"
  }
}