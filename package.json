{
  "name": "corvid-types",
  "version": "0.1.101",
  "description": "Type definitions for Corvid by Wix",
  "scripts": {
    "lint": "eslint .",
    "build": "npm run generate-dts && npm run generate-full-corvid-types && npm run generate-module-list && webpack",
    "test": "jest",
    "test:debug": "node --inspect node_modules/.bin/jest",
    "generate-dts": "scripts/generate-types.js",
    "generate-full-corvid-types": "scripts/generate-full-corvid-types.js",
    "generate-module-list": "scripts/generate-wix-module-list.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix-incubator/corvid-types"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "browser": "dist/corvidTypes.esm.js",
  "main": "src/index.js",
  "files": [
    "src",
    "dist",
    "types",
    "configs",
    "fullCorvidTypes.json",
    "wixModules.json"
  ],
  "types": "./types/index.d.ts",
  "keywords": [
    "corvid",
    "corvid-cli",
    "wix"
  ],
  "dependencies": {
    "@types/node": "^10.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.3",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@commitlint/travis-cli": "^8.1.0",
    "@purtuga/esm-webpack-plugin": "^1.2.1",
    "babel-loader": "^8.0.6",
    "docworks-cli": "^1.10.4",
    "docworks-dts": "^1.10.3",
    "docworks-repo": "^1.9.1",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-prettier": "^3.1.0",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "raw-loader": "^4.0.0",
    "shelljs": "^0.8.3",
    "tmp": "^0.1.0",
    "typescript": "3.7.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  }
}
